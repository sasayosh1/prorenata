        {
          "_key": "reference-span-link-9572c304-c529-4f26-8d2d-a71401e531b5",
          "_type": "span",
          "marks": [
            "link-96be5fd8-8d9f-48d0-9052-b5bf846b59f2"
          ],
          "text": "厚生労働省 統計情報・白書"
        }
      ],
      "markDefs": [
